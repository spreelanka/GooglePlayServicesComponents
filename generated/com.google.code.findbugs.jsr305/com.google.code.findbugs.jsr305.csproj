<Project Sdk="Xamarin.Legacy.Sdk">
  <PropertyGroup>
    <TargetFrameworks>MonoAndroid12.0;net6.0-android31</TargetFrameworks>
    <IsBindingProject>true</IsBindingProject>
    <!--
      No warnings for:
       - CS0618: 'member' is obsolete: 'text'
       - CS0109: The member 'member' does not hide an inherited member. The new keyword is not required
       - CS0114: 'function1' hides inherited member 'function2'. To make the current method override that implementation, add the override keyword. Otherwise add the new keyword.
       - CS0628: 'member' : new protected member declared in sealed class
       - CS0108: 'member1' hides inherited member 'member2'. Use the new keyword if hiding was intended.
       - CS0809: Obsolete member 'member' overrides non-obsolete member 'member'
    -->
    <NoWarn>0618;0109;0114;0628;0108;0809</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Microsoft</Authors>
    <Owners>Microsoft</Owners>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageProjectUrl>https://go.microsoft.com/fwlink/?linkid=865435</PackageProjectUrl>
    <RepositoryUrl>https://go.microsoft.com/fwlink/?linkid=865435</RepositoryUrl>
    <RequireLicenceAcceptance>true</RequireLicenceAcceptance>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT AND Apache-2.0</PackageLicenseExpression>

      <PackageId>Xamarin.Google.Code.FindBugs.JSR305</PackageId>
      <RootNamespace>Xamarin.Google.Code.FindBugs.JSR305</RootNamespace>
            <AssemblyName>Jsr305Binding</AssemblyName>
            <Title>Xamarin.Google.Code.FindBugs.JSR305</Title>
            <PackageDescription>
              Bindings for .NET for Android (formerly Xamarin.Android) Xamarin.Google.Code.FindBugs.JSR305 package
            </PackageDescription>
            <PackageTags>.NET, dotnet, xamarin, android, bindings, google, code, findbugs, jsr305, artifact=com.google.code.findbugs:jsr305 artifact_versioned=com.google.code.findbugs:jsr305:3.0.2</PackageTags>
      <PackageVersion>3.0.2.6</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <TransformFile Include="..\..\source\com.google.code.findbugs\jsr305\Transforms\*.xml" Link="Transforms\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\source\com.google.code.findbugs\jsr305\Additions\*.cs" Link="Additions\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <AndroidLibrary 
        Include="..\..\externals\com.google.code.findbugs\jsr305.aar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\com.google.code.findbugs\jsr305.aar')"
         />
    <Embeddedjar 
        Include="..\..\externals\com.google.code.findbugs\jsr305.jar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\com.google.code.findbugs\jsr305.jar')"
        />
    <JavaSourcesJar Include="..\..\externals\com.google.code.findbugs\jsr305-sources.jar" Link="Jars\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\templates\findbugs\LICENSE.md" Pack="true" PackagePath="LICENSE.md" />
    <None Include="..\..\templates\findbugs\External-Dependency-Info.txt" Pack="true" PackagePath="THIRD-PARTY-NOTICES.txt" />
  </ItemGroup>

</Project>
