

<Project Sdk="Xamarin.Legacy.Sdk">
  <PropertyGroup>
    <TargetFrameworks>MonoAndroid12.0;net6.0-android31.0</TargetFrameworks>
    <IsBindingProject>true</IsBindingProject>
    <AssemblyName>Xamarin.Google.Android.Play.Core.Ktx</AssemblyName>
  </PropertyGroup>


  <PropertyGroup>
    <PackageId>Xamarin.Google.Android.Play.Core.Ktx</PackageId>
    <Title>.NET for Android (formerly Xamarin.Android) Bindings for Google Play Services -  Xamarin.Google.Android.Play.Core.Ktx</Title>
    <Summary>
      .NET for Android (formerly Xamarin.Android) Bindings for Google Play Services - `Xamarin.Google.Android.Play.Core.Ktx` `1.8.1`
    </Summary>
    <Description>
        .NET for Android (formerly Xamarin.Android) bindings for Google Play Services library 'com.google.android.play:core-ktx'.
        
        N/A
    </Description>
    <PackageTags> .NET for Android Xamarin.Android Bindings for Google Play Services artifact=com.google.android.play:core-ktx:.8.1 artifact_versioned=com.google.android.play:core-ktx:.8.1</PackageTags>
    <Authors>Microsoft</Authors>
    <Owners>Microsoft</Owners>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/xamarin/GooglePlayServicesComponents</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/xamarin/GooglePlayServicesComponents/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageIconUrl>N/A</PackageIconUrl>
    <PackageVersion>1.8.1</PackageVersion>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <!-- Include symbol files (*.pdb) in the built .nupkg -->
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <PropertyGroup>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
  </PropertyGroup>

  <PropertyGroup>
    <!--
      =================================================================================================================
      CS warnings (from AndroidX)
    -->
    <!--
      No warnings for:
       - CS0618: 'member' is obsolete: 'text'
       - CS0109: The member 'member' does not hide an inherited member. The new keyword is not required
       - CS0114: 'function1' hides inherited member 'function2'. To make the current method override that implementation, add the override keyword. Otherwise add the new keyword.
       - CS0628: 'member' : new protected member declared in sealed class
       - CS0108: 'member1' hides inherited member 'member2'. Use the new keyword if hiding was intended.
       - CS0809: Obsolete member 'member' overrides non-obsolete member 'member'
    -->
    <NoWarn>0618;0109;0114;0628;0108;0809</NoWarn>
    <!--
      CS warnings (from AndroidX)
      =================================================================================================================
    -->
    <!--
      =================================================================================================================
      Xamarin.Android Bindings specific warnings
    -->
    <!--
      Ignoreable
      Performance hit for builds
      - BG8A04: <attr path="XPath" /> matched no nodes.
      - BG8A00: <remove-node path="XPath" /> matched no nodes.
    -->
    <NoWarn>BG8A04;BG8A00</NoWarn>

    <!--
      Harmfull
      - BG8401: Skipping managed_type, due to a duplicate field, method or nested type name. (Nested type) (Java type: java_type)
      - BG8604: top ancestor Type1 not found for nested type Namespace.Type1.Type2
      - BG8C00: For type Namespace.Type1, base interface java.Interface does not exist
      - BG8700: Unknown return type java.Type1 in method Method1 in managed type Namespace.Type2.
      - BG8800: Unknown parameter type java.Type1 in method Method2 in managed type Namespace.Type2.

    <WarningsAsErrors>BG8401;BG8604;BG8C00;BG8700;BG8800</WarningsAsErrors>
    -->
    <!--
      Xamarin.Android specific warnings
      =================================================================================================================
    -->
  </PropertyGroup>

  <ItemGroup>
      <TransformFile Include="..\..\externals\com.google.android.play\core-ktx-paramnames.xml" Condition="Exists('..\..\externals\com.google.android.play\core-ktx-paramnames.xml')" />
        </ItemGroup>

  <ItemGroup>
    <JavaSourceJar
          Include="..\..\externals\com.google.android.play\core-ktx-sources.jar"
          Condition="Exists('..\..\..\externals\com.google.android.play\core-ktx-sources.jar')"
          />
    <JavaDocJar
          Include="..\..\externals\com.google.android.play\core-ktx-javadoc.jar"
          Condition="Exists('..\..\..\externals\com.google.android.play\core-ktx-javadoc.jar')"
          />
  </ItemGroup>

  <ItemGroup>
    <_AndroidDocumentationPath Include="..\..\externals\paramnames.txt" Condition="Exists('..\..\externals\paramnames.txt')" />
    <_AndroidDocumentationPath Include="..\..\externals\com.google.android.play\core-ktx-paramnames.txt" Condition="Exists('..\..\externals\com.google.android.play\core-ktx-paramnames.txt')" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Xamarin.Google.Android.Play.Core.Ktx.targets" Pack="True" PackagePath="build\MonoAndroid12.0" />
    <None Include="Xamarin.Google.Android.Play.Core.Ktx.targets" Pack="True" PackagePath="build\net6.0-android31.0" />
    <None Include="Xamarin.Google.Android.Play.Core.Ktx.targets" Pack="True" PackagePath="buildTransitive\MonoAndroid12.0" />
    <None Include="Xamarin.Google.Android.Play.Core.Ktx.targets" Pack="True" PackagePath="buildTransitive\net6.0-android31.0" />    
  </ItemGroup>


  <ItemGroup>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Additions\" />
    <Folder Include="Jars\" />
    <Folder Include="Transforms\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\source\AssemblyInfo.cs" />
    <Compile Include="..\..\source\com.google.android.play\core-ktx\Additions\*.cs">
        <Link>Additions/%(RecursiveDir)/%(Filename)%(Extension)</Link>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <TransformFile Include="..\..\source\Metadata.Common.xml" >
      <Link>Transforms/Metadata.Common.xml</Link>
    </TransformFile>
    <TransformFile Include="..\..\source\com.google.android.play\core-ktx\Transforms\*.xml">
        <Link>Transforms/%(RecursiveDir)/%(Filename)%(Extension)</Link>
    </TransformFile>
  </ItemGroup>

  <ItemGroup>
  </ItemGroup>


  <ItemGroup>
    <InputJar Include="..\..\externals\com.google.android.play\core-ktx\classes.jar" />
    <!-- For those artifacts with lib/ folder -->
    <InputJar
      Condition="Exists('..\..\externals\com.google.android.play\core-ktx\libs\')"
      Include="..\..\externals\com.google.android.play\core-ktx\libs\*.jar"
      />
    <!-- Pull in the play core extensions helpers -->
  </ItemGroup>
  <ItemGroup>
    <!-- ProjectReference -->
  </ItemGroup>

  <ItemGroup>
    <!-- ProjectReference -->
        <ProjectReference Include="..\..\generated\com.google.android.play.core\com.google.android.play.core.csproj" PrivateAssets="none" />
    <!-- some additional fixes -->
  </ItemGroup>

  <ItemGroup>
    <!-- PackageReference -->
      <PackageReference Include="Xamarin.Kotlin.StdLib.Jdk7" Version="1.6.21.1" PrivateAssets="none" />
      <PackageReference Include="Xamarin.KotlinX.Coroutines.Android" Version="1.5.2.5" PrivateAssets="none" />
      <PackageReference Include="Xamarin.AndroidX.Core" Version="1.7.0.2" PrivateAssets="none" />
      <PackageReference Include="Xamarin.AndroidX.Fragment" Version="1.4.1.1" PrivateAssets="none" />
                    <!-- Reference -->
    <Reference Include="System.Net.Http" Condition=" '$(TargetFramework)' == 'MonoAndroid12.0' " />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Xamarin.Build.Download" Version="0.11.4" />
  </ItemGroup>

  <Import Project="$(MSBuildSDKExtrasTargets)" Condition="Exists('$(MSBuildSDKExtrasTargets)')" />

</Project>

