<Project Sdk="Xamarin.Legacy.Sdk">
  <PropertyGroup>
    <TargetFrameworks>MonoAndroid12.0;net6.0-android</TargetFrameworks>
    <IsBindingProject>true</IsBindingProject>
    <!--
      No warnings for:
       - CS0618: 'member' is obsolete: 'text'
       - CS0109: The member 'member' does not hide an inherited member. The new keyword is not required
       - CS0114: 'function1' hides inherited member 'function2'. To make the current method override that implementation, add the override keyword. Otherwise add the new keyword.
       - CS0628: 'member' : new protected member declared in sealed class
       - CS0108: 'member1' hides inherited member 'member2'. Use the new keyword if hiding was intended.
       - CS0809: Obsolete member 'member' overrides non-obsolete member 'member'
    -->
    <NoWarn>0618;0109;0114;0628;0108;0809</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Microsoft</Authors>
    <Owners>Microsoft</Owners>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageProjectUrl>https://go.microsoft.com/fwlink/?linkid=865435</PackageProjectUrl>
    <RepositoryUrl>https://go.microsoft.com/fwlink/?linkid=865435</RepositoryUrl>
    <RequireLicenceAcceptance>true</RequireLicenceAcceptance>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT AND Apache-2.0</PackageLicenseExpression>

      <PackageId>Square.OkHttp3.UrlConnection</PackageId>
      <RootNamespace>Square.OkHttp3.UrlConnection</RootNamespace>
      <AssemblyName>Square.OkHttp3.UrlConnection</AssemblyName>
            <Title>OkHttp v3 UrlConnection bindings for .NET for Android (formerly Xamarin.Android)</Title>
            <Summary>
              OkHttp ability to be used with HttpURLConnection.
            </Summary>
            <Description>
              OkHttp ability to be used with HttpURLConnection.
            </Description>
            <PackageDescription>
              OkHttp ability to be used with HttpURLConnection.
            </PackageDescription>
            <PackageTags>.NET dotnet okhttp square square.okhttp xamarin android monodroid artifact=com.squareup.okhttp3:okhttp-urlconnection artifact_versioned=com.squareup.okhttp3:okhttp-urlconnection:4.9.3</PackageTags>
      <PackageVersion>4.9.3.2</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <TransformFile Include="..\..\source\com.squareup.okhttp3\okhttp-urlconnection\Transforms\*.xml" Link="Transforms\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\source\com.squareup.okhttp3\okhttp-urlconnection\Additions\*.cs" Link="Additions\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <AndroidLibrary 
        Include="..\..\externals\com.squareup.okhttp3\okhttp-urlconnection.aar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\com.squareup.okhttp3\okhttp-urlconnection.aar')"
         />
    <Embeddedjar 
        Include="..\..\externals\com.squareup.okhttp3\okhttp-urlconnection.jar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\com.squareup.okhttp3\okhttp-urlconnection.jar')"
        />
    <JavaSourcesJar Include="..\..\externals\com.squareup.okhttp3\okhttp-urlconnection-sources.jar" Link="Jars\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\generated\com.squareup.okhttp3.okhttp\com.squareup.okhttp3.okhttp.csproj" PrivateAssets="none" />
    <PackageReference Include="Xamarin.Kotlin.StdLib.Jdk8" Version="1.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\templates\squareup-okhttp3\License.md" Pack="true" PackagePath="LICENSE.md" />
    <None Include="..\..\templates\squareup-okhttp3\External-Dependency-Info.txt" Pack="true" PackagePath="THIRD-PARTY-NOTICES.txt" />
  </ItemGroup>

</Project>
