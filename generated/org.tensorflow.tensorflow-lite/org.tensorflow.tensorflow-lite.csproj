<Project Sdk="Xamarin.Legacy.Sdk">
  <PropertyGroup>
    <TargetFrameworks>MonoAndroid12.0;net6.0-android31</TargetFrameworks>
    <IsBindingProject>true</IsBindingProject>
    <!--
      No warnings for:
       - CS0618: 'member' is obsolete: 'text'
       - CS0109: The member 'member' does not hide an inherited member. The new keyword is not required
       - CS0114: 'function1' hides inherited member 'function2'. To make the current method override that implementation, add the override keyword. Otherwise add the new keyword.
       - CS0628: 'member' : new protected member declared in sealed class
       - CS0108: 'member1' hides inherited member 'member2'. Use the new keyword if hiding was intended.
       - CS0809: Obsolete member 'member' overrides non-obsolete member 'member'
    -->
    <NoWarn>0618;0109;0114;0628;0108;0809</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Microsoft</Authors>
    <Owners>Microsoft</Owners>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageProjectUrl>https://go.microsoft.com/fwlink/?linkid=865435</PackageProjectUrl>
    <RepositoryUrl>https://go.microsoft.com/fwlink/?linkid=865435</RepositoryUrl>
    <RequireLicenceAcceptance>true</RequireLicenceAcceptance>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT AND Apache-2.0</PackageLicenseExpression>

      <PackageId>Xamarin.TensorFlow.Lite</PackageId>
            <RootNamespace>Xamarin.TensorFlow.Lite</RootNamespace>
            <AssemblyName>Xamarin.TensorFlow.Lite</AssemblyName>
            <Title>Xamarin.TensorFlow.Lite</Title>
            <Summary>
              .NET for Android (formerly Xamarin.Android) Bindings for Google's TensorFlow Lite package (Google Play Services dependency)
            </Summary>
            <Description>
              .NET for Android (formerly Xamarin.Android) Bindings for Google's TensorFlow Lite package (Google Play Services dependency)
            </Description>
            <PackageDescription>
              .NET for Android (formerly Xamarin.Android) Bindings for Google's TensorFlow Lite package (Google Play Services dependency)
            </PackageDescription>
            <PackageTags>.NET, dotnet, xamarin, android, bindings, google, tensorflow, lite, artifact=org.tensorflow:tensorflow-lite artifact_versioned=org.tensorflow:tensorflow-lite:2.10.0</PackageTags>
      <PackageVersion>2.10.0</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <TransformFile Include="..\..\source\org.tensorflow\tensorflow-lite\Transforms\*.xml" Link="Transforms\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\source\org.tensorflow\tensorflow-lite\Additions\*.cs" Link="Additions\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <AndroidLibrary 
        Include="..\..\externals\org.tensorflow\tensorflow-lite.aar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\org.tensorflow\tensorflow-lite.aar')"
         />
    <Embeddedjar 
        Include="..\..\externals\org.tensorflow\tensorflow-lite.jar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\org.tensorflow\tensorflow-lite.jar')"
        />
    <JavaSourcesJar Include="..\..\externals\org.tensorflow\tensorflow-lite-sources.jar" Link="Jars\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\generated\org.tensorflow.tensorflow-lite-api\org.tensorflow.tensorflow-lite-api.csproj" PrivateAssets="none" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\templates\tensorflow-lite\LICENSE.md" Pack="true" PackagePath="LICENSE.md" />
    <None Include="..\..\templates\tensorflow-lite\External-Dependency-Info.txt" Pack="true" PackagePath="THIRD-PARTY-NOTICES.txt" />
  </ItemGroup>

</Project>
