<Project Sdk="Xamarin.Legacy.Sdk">
  <PropertyGroup>
    <TargetFrameworks>MonoAndroid12.0;net6.0-android31</TargetFrameworks>
    <IsBindingProject>true</IsBindingProject>
    <!--
      No warnings for:
       - CS0618: 'member' is obsolete: 'text'
       - CS0109: The member 'member' does not hide an inherited member. The new keyword is not required
       - CS0114: 'function1' hides inherited member 'function2'. To make the current method override that implementation, add the override keyword. Otherwise add the new keyword.
       - CS0628: 'member' : new protected member declared in sealed class
       - CS0108: 'member1' hides inherited member 'member2'. Use the new keyword if hiding was intended.
       - CS0809: Obsolete member 'member' overrides non-obsolete member 'member'
    -->
    <NoWarn>0618;0109;0114;0628;0108;0809</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Microsoft</Authors>
    <Owners>Microsoft</Owners>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageProjectUrl>https://go.microsoft.com/fwlink/?linkid=865435</PackageProjectUrl>
    <RepositoryUrl>https://go.microsoft.com/fwlink/?linkid=865435</RepositoryUrl>
    <RequireLicenceAcceptance>true</RequireLicenceAcceptance>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT AND Apache-2.0</PackageLicenseExpression>

      <PackageId>Xamarin.Google.Dagger</PackageId>
      <RootNamespace>Xamarin.Google.Dagger</RootNamespace>
      <AssemblyName>Xamarin.Google.Dagger</AssemblyName>
            <Title>Xamarin.Google.Dagger</Title>
            <PackageDescription>.NET for Android (formerly Xamarin.Android) bindings for Google Dagger</PackageDescription>
            <Summary>.NET for Android (formerly Xamarin.Android) bindings for Google Dagger</Summary>
            <PackageTags>.NET, dotnet, xamarin, android, bindings, google dagger artifact=com.google.dagger:dagger artifact_versioned=com.google.dagger:dagger:2.41</PackageTags>
      <PackageVersion>2.41.0.2</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <TransformFile Include="..\..\source\com.google.dagger\dagger\Transforms\*.xml" Link="Transforms\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\source\com.google.dagger\dagger\Additions\*.cs" Link="Additions\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <AndroidLibrary 
        Include="..\..\externals\com.google.dagger\dagger.aar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\com.google.dagger\dagger.aar')"
         />
    <Embeddedjar 
        Include="..\..\externals\com.google.dagger\dagger.jar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\com.google.dagger\dagger.jar')"
        />
    <JavaSourcesJar Include="..\..\externals\com.google.dagger\dagger-sources.jar" Link="Jars\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\generated\javax.inject.javax.inject\javax.inject.javax.inject.csproj" PrivateAssets="none" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\templates\dagger\License.md" Pack="true" PackagePath="License.md" />
    <None Include="..\..\templates\dagger\External-Dependency-Info.txt" Pack="true" PackagePath="THIRD-PARTY-NOTICES.txt" />
  </ItemGroup>

</Project>
