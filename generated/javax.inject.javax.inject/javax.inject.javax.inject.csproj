<Project Sdk="Xamarin.Legacy.Sdk">
  <PropertyGroup>
    <TargetFrameworks>MonoAndroid12.0;net6.0-android31</TargetFrameworks>
    <IsBindingProject>true</IsBindingProject>
    <!--
      No warnings for:
       - CS0618: 'member' is obsolete: 'text'
       - CS0109: The member 'member' does not hide an inherited member. The new keyword is not required
       - CS0114: 'function1' hides inherited member 'function2'. To make the current method override that implementation, add the override keyword. Otherwise add the new keyword.
       - CS0628: 'member' : new protected member declared in sealed class
       - CS0108: 'member1' hides inherited member 'member2'. Use the new keyword if hiding was intended.
       - CS0809: Obsolete member 'member' overrides non-obsolete member 'member'
    -->
    <NoWarn>0618;0109;0114;0628;0108;0809</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Microsoft</Authors>
    <Owners>Microsoft</Owners>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageProjectUrl>https://go.microsoft.com/fwlink/?linkid=865435</PackageProjectUrl>
    <RepositoryUrl>https://go.microsoft.com/fwlink/?linkid=865435</RepositoryUrl>
    <RequireLicenceAcceptance>true</RequireLicenceAcceptance>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT AND Apache-2.0</PackageLicenseExpression>

      <PackageId>Xamarin.JavaX.Inject</PackageId>
      <RootNamespace>Xamarin.JavaX.Inject</RootNamespace>
      <AssemblyName>Xamarin.JavaX.Inject</AssemblyName>
            <Title>JavaX Inject bindings for Xamarin.Android</Title>
            <Summary>.NET for Android (formerly Xamarin.Android) bindings for Javax Inject</Summary>
            <PackageDescription>.NET for Android (formerly Xamarin.Android) bindings for JavaX Inject</PackageDescription>
            <PackageTags>.NET, dotnet, xamarin, android, bindings, javax, inject, artifact=javax.inject:javax.inject artifact_versioned=javax.inject:javax.inject:1</PackageTags>
      <PackageVersion>1.0.0.6</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <TransformFile Include="..\..\source\javax.inject\javax.inject\Transforms\*.xml" Link="Transforms\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\source\javax.inject\javax.inject\Additions\*.cs" Link="Additions\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <AndroidLibrary 
        Include="..\..\externals\javax.inject\javax.inject.aar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\javax.inject\javax.inject.aar')"
         />
    <Embeddedjar 
        Include="..\..\externals\javax.inject\javax.inject.jar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\javax.inject\javax.inject.jar')"
        />
    <JavaSourcesJar Include="..\..\externals\javax.inject\javax.inject-sources.jar" Link="Jars\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\templates\javax-inject\License.md" Pack="true" PackagePath="LICENSE.md" />
    <None Include="..\..\templates\javax-inject\External-Dependency-Info.txt" Pack="true" PackagePath="THIRD-PARTY-NOTICES.txt" />
  </ItemGroup>

</Project>
