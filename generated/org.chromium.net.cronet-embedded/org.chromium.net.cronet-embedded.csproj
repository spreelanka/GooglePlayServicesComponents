<Project Sdk="Xamarin.Legacy.Sdk">
  <PropertyGroup>
    <TargetFrameworks>MonoAndroid12.0;net6.0-android31</TargetFrameworks>
    <IsBindingProject>true</IsBindingProject>
    <!--
      No warnings for:
       - CS0618: 'member' is obsolete: 'text'
       - CS0109: The member 'member' does not hide an inherited member. The new keyword is not required
       - CS0114: 'function1' hides inherited member 'function2'. To make the current method override that implementation, add the override keyword. Otherwise add the new keyword.
       - CS0628: 'member' : new protected member declared in sealed class
       - CS0108: 'member1' hides inherited member 'member2'. Use the new keyword if hiding was intended.
       - CS0809: Obsolete member 'member' overrides non-obsolete member 'member'
    -->
    <NoWarn>0618;0109;0114;0628;0108;0809</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Microsoft</Authors>
    <Owners>Microsoft</Owners>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageProjectUrl>https://go.microsoft.com/fwlink/?linkid=865435</PackageProjectUrl>
    <RepositoryUrl>https://go.microsoft.com/fwlink/?linkid=865435</RepositoryUrl>
    <RequireLicenceAcceptance>true</RequireLicenceAcceptance>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT AND Apache-2.0</PackageLicenseExpression>

      <PackageId>Xamarin.Chromium.CroNet.Embedded</PackageId>
      <RootNamespace>Xamarin.Chromium.CroNet.Embedded</RootNamespace>
      <AssemblyName>Xamarin.Chromium.CroNet.Embedded</AssemblyName>
            <Title>Xamarin.Chromium.CroNet.Embedded</Title>
            <PackageDescription>
              Bindings for .NET for Android (formerly Xamarin.Android) Chromium.CroNet.Embedded package
            </PackageDescription>
            <PackageTags>.NET, dotnet, xamarin, android, bindings, chromium, cronet, embedded artifact=org.chromium.net:cronet-embedded artifact_versioned=org.chromium.net:cronet-embedded:92.4515.131</PackageTags>
      <PackageVersion>92.4515.131.2</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <TransformFile Include="..\..\source\org.chromium.net\cronet-embedded\Transforms\*.xml" Link="Transforms\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\source\org.chromium.net\cronet-embedded\Additions\*.cs" Link="Additions\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <LibraryProjectZip 
        Include="..\..\externals\org.chromium.net\cronet-embedded.aar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\org.chromium.net\cronet-embedded.aar')"
         />
    <Embeddedjar 
        Include="..\..\externals\org.chromium.net\cronet-embedded.jar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\org.chromium.net\cronet-embedded.jar')"
        />
    <JavaSourcesJar Include="..\..\externals\org.chromium.net\cronet-embedded-sources.jar" Link="Jars\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\generated\org.chromium.net.cronet-common\org.chromium.net.cronet-common.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\generated\com.google.code.findbugs.jsr305\com.google.code.findbugs.jsr305.csproj" PrivateAssets="none" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\templates\chromium-cronet\LICENSE.md" Pack="true" PackagePath="LICENSE.md" />
    <None Include="..\..\templates\chromium-cronet\External-Dependency-Info.txt" Pack="true" PackagePath="THIRD-PARTY-NOTICES.txt" />
  </ItemGroup>

</Project>
