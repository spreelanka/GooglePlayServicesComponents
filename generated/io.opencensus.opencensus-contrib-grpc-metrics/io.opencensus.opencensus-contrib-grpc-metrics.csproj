<Project Sdk="Xamarin.Legacy.Sdk">
  <PropertyGroup>
    <TargetFrameworks>MonoAndroid12.0;net6.0-android31</TargetFrameworks>
    <IsBindingProject>true</IsBindingProject>
    <!--
      No warnings for:
       - CS0618: 'member' is obsolete: 'text'
       - CS0109: The member 'member' does not hide an inherited member. The new keyword is not required
       - CS0114: 'function1' hides inherited member 'function2'. To make the current method override that implementation, add the override keyword. Otherwise add the new keyword.
       - CS0628: 'member' : new protected member declared in sealed class
       - CS0108: 'member1' hides inherited member 'member2'. Use the new keyword if hiding was intended.
       - CS0809: Obsolete member 'member' overrides non-obsolete member 'member'
    -->
    <NoWarn>0618;0109;0114;0628;0108;0809</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Microsoft</Authors>
    <Owners>Microsoft</Owners>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageProjectUrl>https://go.microsoft.com/fwlink/?linkid=865435</PackageProjectUrl>
    <RepositoryUrl>https://go.microsoft.com/fwlink/?linkid=865435</RepositoryUrl>
    <RequireLicenceAcceptance>true</RequireLicenceAcceptance>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT AND Apache-2.0</PackageLicenseExpression>

      <PackageId>Xamarin.Io.OpenCensus.OpenCensusContribGrpcMetrics</PackageId>
      <RootNamespace>Xamarin.Io.OpenCensus.OpenCensusContribGrpcMetrics</RootNamespace>
      <AssemblyName>Xamarin.Io.OpenCensus.OpenCensusContribGrpcMetrics</AssemblyName>
            <Title>Xamarin.Io.OpenCensus.OpenCensusContribGrpcMetrics</Title>
            <PackageDescription>.NET for Android (formerly Xamarin.Android) Bindings for Io.OpenCensus.OpenCensusContribGrpcMetrics io.opencensus:opencensus-contrib-grpc-metrics package (Google  GRPC  dependency)</PackageDescription>
            <RequireLicenceAcceptance>true</RequireLicenceAcceptance>
            <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
            <Summary>.NET for Android (formerly Xamarin.Android) Bindings for io.opencensus:opencensus-contrib-grpc-metrics package</Summary>
            <PackageTags>.NET, dotnet, xamarin, android, bindings, io.opencensus, io, opencensus, contrib-grpc-metrics, artifact=io.opencensus:opencensus-contrib-grpc-metrics artifact_versioned=io.opencensus:opencensus-contrib-grpc-metrics:0.29.0</PackageTags>
      <PackageVersion>0.29.0.2</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <TransformFile Include="..\..\source\io.opencensus\opencensus-contrib-grpc-metrics\Transforms\*.xml" Link="Transforms\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\source\io.opencensus\opencensus-contrib-grpc-metrics\Additions\*.cs" Link="Additions\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <AndroidLibrary 
        Include="..\..\externals\io.opencensus\opencensus-contrib-grpc-metrics.aar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\io.opencensus\opencensus-contrib-grpc-metrics.aar')"
         />
    <Embeddedjar 
        Include="..\..\externals\io.opencensus\opencensus-contrib-grpc-metrics.jar" Link="Jars\%(Filename)%(Extension)" 
        Condition="Exists('..\..\externals\io.opencensus\opencensus-contrib-grpc-metrics.jar')"
        />
    <JavaSourcesJar Include="..\..\externals\io.opencensus\opencensus-contrib-grpc-metrics-sources.jar" Link="Jars\%(Filename)%(Extension)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\generated\io.opencensus.opencensus-api\io.opencensus.opencensus-api.csproj" PrivateAssets="none" />
    <PackageReference Include="Xamarin.Google.Guava" Version="31.1.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\templates\opencensus\LICENSE.md" Pack="true" PackagePath="LICENSE.md" />
    <None Include="..\..\templates\opencensus\External-Dependency-Info.txt" Pack="true" PackagePath="THIRD-PARTY-NOTICES.txt" />
  </ItemGroup>

</Project>
